{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ES2022",
        "module": "commonjs",
        "lib": [
            "ES2022",
            "dom"
        ],
        "declaration": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": false,
        "inlineSourceMap": true,
        "inlineSources": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "strictPropertyInitialization": false,
        "skipDefaultLibCheck": true,
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "exclude": [
        "cdk.out",
        "dist",
        "node_modules",
        "test",
        ".git",
        ".idea",
        ".vscode",
        "cdk.context.json"
    ],
    "include": [
        "lib"
    ]
}